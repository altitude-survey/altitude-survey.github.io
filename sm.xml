---
layout: none
permalink: /sm.xml
sitemap: false
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
    {%- assign sort_pages = site.pages | sort: "sortid"  -%}
    {%- for p in sort_pages -%}
        {%- if p.title != nil and pa.cat and p.sitemap != false -%}
        <url>
            <loc>{{ site.url }}{{ p.url | uri_escape }}</loc>
            <lastmod>{{ site.time | date_to_xmlschema }}</lastmod>
        </url>
        {%- endif -%}
    {%- endfor -%}
    {%- for d in site.documents -%}
        {%- if d.sitemap != false -%}
        <url>
            <loc>{{ site.url }}{{ d.url | uri_escape }}</loc>
            <lastmod>{{ site.time | date_to_xmlschema }}</lastmod>
        </url>
        {%- endif -%}
    {%- endfor -%}  
</urlset>